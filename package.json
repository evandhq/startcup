  },
  "homepage": "https://github.com/mohebifar/startcup#readme",
  "dependencies": {
    "babel-polyfill": "^6.8.0",
    "body-parser": "^1.15.1",
    "concurrently": "^2.0.0",
    "express": "^4.13.4",
    "mongoose": "^4.4.15",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-redux": "^4.4.5",
