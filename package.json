{
  "name": "startcup",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npm run eslint",
    "dev": "concurrent --kill-others \"npm run webpack-dev\" \"npm run start-dev\"",
    "webpack-dev": "set NODE_ENV=development && set NODE_PATH=./src && node webpack/webpack-dev-server.js",
    "start-dev": "NODE_ENV=development NODE_PATH=./src node ./bin/server.js",
    "start": "NODE_ENV=production NODE_PATH=./src node ./bin/server.js",
    "build": "node ./node_modules/.bin/webpack --colors --config ./webpack/prod.config.js",
    "eslint": "eslint ./src",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
